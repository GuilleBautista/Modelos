<?xml version="1.0" encoding="UTF-8"?>
<conversationalGame:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:conversationalGame="http://www.example.org/var"
    xsi:schemaLocation="http://www.example.org/var dsl.ecore"
    SpawnRoom="//@RoomList.0"
    Greeting="Estás listo para tu aventura pokémon?"
    ItemList="//@RoomList.1/@Items.0 //@RoomList.4/@Items.0 //@RoomList.3/@Npcs.0/@Inventory.0"
    NpcList="//@RoomList.1/@Npcs.0 //@RoomList.3/@Npcs.0 //@RoomList.4/@Npcs.0"
    CharacterActionList="//@Player/@Actions.0">
  <Player CurrentRoom="//@RoomList.0">
    <Actions
        Name="move"
        Description="Move"
        Id="MovePlayer">
      <Consequence
          xsi:type="conversationalGame:PlayerConsequence"
          Message="You move through the door"
          Player="//@Player"/>
      <Consequence
          xsi:type="conversationalGame:StatConsequence"
          Message="foo"
          Character="//@Player"/>
    </Actions>
  </Player>
  <RoomList
      Id="OwnRoom">
    <East Next="//@RoomList.1"
        Id="OwnRoomToHouse"/>
  </RoomList>
  <RoomList
      Id="House">
    <East Next="//@RoomList.2"
        Id="HouseToPalletTown"/>
    <West Next="//@RoomList.0"
        Id="HouseToOwnRoom"/>
    <Items Name="Orange Juice"
        Id="i1"/>
    <Npcs Id="Mom"
        Name="Mom"/>
  </RoomList>
  <RoomList
      Id="PalletTown">
    <North Open="false"
        Next="//@RoomList.4"
        Id="PalletTownToWildArea"/>
    <East Next="//@RoomList.3"
        Id="PalletTownToOakLab"/>
    <West Next="//@RoomList.1"
        Id="PalletTownToHouse"/>
  </RoomList>
  <RoomList
      Id="OakLab">
    <West Next="//@RoomList.2"
        Id="OakLabToPalletTown"/>
    <Npcs Id="Oak"
        Name="OakProfessor">
      <Inventory
          Name="Pokedex"
          Id="Pokedex"/>
    </Npcs>
  </RoomList>
  <RoomList
      Id="WildArea">
    <South Next="//@RoomList.2"
        Id="WildAreaToPalletTown"/>
    <Items Name="Pokeballs"
        Id="Pokeballs"/>
    <Npcs Id="Pikachu"
        Name="Pikachu"/>
  </RoomList>
</conversationalGame:Game>
